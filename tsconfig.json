{
  "compilerOptions": {
    "outDir": "./src",
    "allowJs": true,
    "target": "es5",
    "rootDir": "./src",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*",
    "environment.d.ts"
  ],
  "exclude":[
    "node_modules",
    "**/*.spec.ts"
]
}
