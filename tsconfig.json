{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2020"],
    "outDir": "build",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": false,
    "skipLibCheck": true
},
  "include": [
    "./src/**/*",
    "environment.d.ts"
  ],
  "exclude":[
    "node_modules",
    "**/*.spec.ts"
]
}
